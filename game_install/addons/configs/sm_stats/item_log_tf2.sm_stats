create table if not exists `item_log_tf2`
(
	`ID` int not null auto_increment,
	`ServerID` int not null default '1',
	`Timestamp` int not null default '0',
	`itemdef` int not null default '-1',
	`quality` int(4) not null default '-1',
	`method` int(4) not null default '-1',
	`isstrange` bool not null default '0',
	`isunusual` bool not null default '0',
	`wear` float not null default '-1',
	`amount` int not null default '0',
	primary key (`ID`)
)